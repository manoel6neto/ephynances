<p id="notice"><%= notice %></p>

<h1>Listing Agreement Documents</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agreement_documents.each do |agreement_document| %>
      <tr>
        <td><%= link_to 'Show', agreement_document %></td>
        <td><%= link_to 'Edit', edit_agreement_document_path(agreement_document) %></td>
        <td><%= link_to 'Destroy', agreement_document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agreement document', new_agreement_document_path %>
